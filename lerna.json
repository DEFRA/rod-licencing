{
  "packages": ["packages/*"],
  "version": "0.0.0",
  "npmClient": "npm",
  "command": {
    "bootstrap": {},
    "version": {
      "message": "chore(release): publish %s"
    },
    "publish": {
      "message": "chore(release): publish %s",
      "allowBranch": "master",
      "registry": "https://npm.pkg.github.com",
      "ignoreChanges": [".*", "*.md", "docker/*", "sonar-project.properties"]
    }
  }
}
